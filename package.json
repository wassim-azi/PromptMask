{
  "name": "promptmask",
  "version": "1.0.0",
  "displayName": "PromptMask",
  "description": "PromptMask replaces personal data in your AI prompts with fake or user-defined placeholder information.",
  "author": "Wassim AZI <wassim@promptmask.ai>",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "package": "plasmo package"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@fontsource/roboto": "^5.2.5",
    "@mui/icons-material": "^7.0.2",
    "@mui/material": "^7.0.2",
    "@plasmohq/messaging": "^0.7.1",
    "@plasmohq/storage": "^1.15.0",
    "formik": "^2.4.6",
    "framer-motion": "^12.7.1",
    "next": "^15.3.0",
    "plasmo": "0.90.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "^5.5.0",
    "yup": "^1.6.1"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.1.1",
    "@types/chrome": "0.0.258",
    "@types/node": "20.11.5",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.3",
    "prettier": "3.2.4",
    "tailwindcss": "^3.4.17",
    "typescript": "5.3.3"
  },
  "manifest": {
    "default_locale": "en",
    "host_permissions": [
      "https://*/*",
      "http://*/*"
    ],
    "permissions": [
      "activeTab",
      "tabs",
      "storage"
    ],
    "externally_connectable": {
      "matches": [
        "*://*.localhost.com/*"
      ]
    }
  },
  "contributors": [
    "Wassim AZI <wassim@promptmask.ai>"
  ]
}
